    "@halo-dev/ui-shared": "^2.22.1",
    "@joplin/turndown-plugin-gfm": "^1.0.64",
    "@tanstack/vue-query": "4",
    "@vueuse/core": "^11.3.0",
    "@vueuse/router": "^11.3.0",
    "axios": "^1.13.4",
    "es-toolkit": "^1.44.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "mammoth": "^1.11.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "turndown": "^7.2.2",
    "vue": "^3.5.27",
    "vue-reactive-queue": "^0.0.1"
  },
  "devDependencies": {
    "@halo-dev/ui-plugin-bundler-kit": "2.22.0",
