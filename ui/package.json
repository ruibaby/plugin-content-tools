{
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild build --env-mode development --watch",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path ../.gitignore",
    "prettier": "prettier --write src/",
    "type-check": "vue-tsc --build --force"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@halo-dev/api-client": "^2.21.1",
    "@halo-dev/components": "^2.21.0",
    "@halo-dev/console-shared": "^2.21.0",
    "@joplin/turndown-plugin-gfm": "^1.0.62",
    "@tanstack/vue-query": "4",
    "@vueuse/core": "^11.3.0",
    "@vueuse/router": "^11.3.0",
    "axios": "^1.11.0",
    "es-toolkit": "^1.39.10",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "mammoth": "^1.10.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "p-queue": "^8.1.0",
    "turndown": "^7.2.1",
    "vue": "^3.5.20"
  },
  "devDependencies": {
    "@halo-dev/ui-plugin-bundler-kit": "2.21.2",
